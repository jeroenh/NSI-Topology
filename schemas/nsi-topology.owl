<?xml version="1.0"?>
<rdf:RDF xmlns="http://schemas.ogf.org/nsi/2013/03/topology#"
     xml:base="http://schemas.ogf.org/nsi/2013/03/topology"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:nml="http://schemas.ogf.org/nml/2013/03/base#"
     xmlns:nsi="http://schemas.ogf.org/nsi/2013/03/topology#">

    <owl:Ontology rdf:about="http://schemas.ogf.org/nsi/2013/03/topology#">
        <owl:imports rdf:resource="http://schemas.ogf.org/nml/2013/03/base#"/>
    </owl:Ontology>

    <!-- http://schemas.ogf.org/nsi/2013/03/topology#adminContact -->

    <owl:ObjectProperty rdf:about="http://schemas.ogf.org/nsi/2013/03/topology#adminContact">
        <rdfs:domain rdf:resource="http://schemas.ogf.org/nml/2013/03/base#NetworkObject"/>
    </owl:ObjectProperty>

    <!-- http://schemas.ogf.org/nsi/2013/03/topology#manages -->

    <owl:ObjectProperty rdf:about="http://schemas.ogf.org/nsi/2013/03/topology#manages">
        <rdfs:domain rdf:resource="http://schemas.ogf.org/nsi/2013/03/topology#NSA"/>
        <rdfs:range rdf:resource="http://schemas.ogf.org/nml/2013/03/base#NetworkObject"/>
    </owl:ObjectProperty>

    <!-- http://schemas.ogf.org/nsi/2013/03/topology#peersWith -->

    <owl:ObjectProperty rdf:about="http://schemas.ogf.org/nsi/2013/03/topology#peersWith">
        <rdfs:domain rdf:resource="http://schemas.ogf.org/nsi/2013/03/topology#NSA"/>
        <rdfs:range rdf:resource="http://schemas.ogf.org/nsi/2013/03/topology#NSA"/>
    </owl:ObjectProperty>
    
    <!-- http://schemas.ogf.org/nsi/2013/03/topology#csProviderEndpoint -->

    <owl:DatatypeProperty rdf:about="http://schemas.ogf.org/nsi/2013/03/topology#csProviderEndpoint">
        <rdfs:domain rdf:resource="http://schemas.ogf.org/nsi/2013/03/topology#NSA"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    </owl:DatatypeProperty>

    <!-- http://schemas.ogf.org/nsi/2013/03/topology#NSA -->

    <owl:Class rdf:about="http://schemas.ogf.org/nsi/2013/03/topology#NSA">
        <rdfs:subClassOf rdf:resource="http://schemas.ogf.org/nml/2013/03/base#NetworkObject"/>
    </owl:Class>
    <owl:Class rdf:about="http://schemas.ogf.org/nsi/2013/03/topology#Service">
        <rdfs:subClassOf rdf:resource="http://schemas.ogf.org/nml/2013/03/base#Service"/>
    </owl:Class>
</rdf:RDF>

